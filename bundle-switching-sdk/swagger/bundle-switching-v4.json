    "/bundle-switching/v4/dossiers/confirmations/{distributionId}": {
      "put": {
        "tags": [
          "COIN Bundle Switching API"
        ],
        "summary": "Confirm that a message has been received",
        "description": "",
        "operationId": "putConfirmation",
        "parameters": [
          {
            "name": "distributionId",
            "in": "path",

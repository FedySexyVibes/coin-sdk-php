    "/bundle-switching/v4/dossiers/events": {
      "get": {
        "tags": [
          "COIN Bundle Switching API"
        ],
        "summary": "Returns server-sent event stream",
        "description": "",
        "operationId": "events",
        "produces": [
          "text/event-stream"
        ],
        "parameters": [
          {
            "name": "offset",
            "in": "query",
            "required": false,
            "type": "integer",
            "default": -1,
            "format": "int64"
          },
          {
            "name": "messageTypes",
            "in": "query",
            "required": false,
            "type": "string",
            "enum": [
              "contractterminationrequest",
              "contractterminationrequestanswer",
              "cancel",
              "contractterminationperformed",
              "errorfound"
            ]
          },
          {
            "name": "confirmationStatus",
            "in": "query",
            "required": false,
            "type": "string",
            "default": "Unconfirmed",
            "enum": [
              "Unconfirmed",
              "All"
            ]
          },
          {
            "name": "serviceproviders",
            "in": "query",
            "required": false,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "successful operation",
            "schema": {
              "$ref": "#/definitions/ServerSentEvent"
            }
          },
          "404": {
            "description": "ServiceProvider not found"
          },
          "400": {
            "description": "ill-formed contract termination message",
            "schema": {
              "$ref": "#/definitions/ErrorMessages"
            }
          },
          "401": {
            "description": "Invalid Hmac",
            "schema": {
